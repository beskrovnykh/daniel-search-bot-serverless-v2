{
  "version": "2.0",
  "app_name": "daniel-search-bot-serverless-v2",
  "api_gateway_endpoint_type": "REGIONAL",
  "environment_variables": {
    "TELEGRAM_TOKEN": "",
    "OPENAI_API_KEY": "",
    "VOICE_MESSAGES_BUCKET": "",
    "PINECONE_API_KEY" : "",
    "PINECONE_ENV" : ""
  },
  "lambda_timeout": 600,
  "stages": {
    "local": {
      "api_gateway_stage": "api",
      "autogen_policy": false,
      "iam_policy_file": "dev-policy.json",
      "environment_variables": {
        "STAGE": "local"
      }
    },
    "dev": {
      "api_gateway_stage": "api",
      "autogen_policy": false,
      "iam_policy_file": "dev-policy.json",
      "environment_variables": {
        "STAGE": "dev"
      }
    }
  }
}